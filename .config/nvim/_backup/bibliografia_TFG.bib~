
@report{blinder_ammonia_2018,
	title = {Ammonia Inversion Energy Levels using Operator Algebra},
	url = {http://arxiv.org/abs/1809.08178},
	abstract = {The inversion potential for the ammonia molecule is approximated by V (x) = k(x2 − r2)2/8r2. The Hamiltonian thereby contains only even powers of p and x and a representation in terms of ladder operators a and a† is suggested. The frequency variable ω occurring in the operators is introduced as a free parameter, with its value to be determined such as to optimize agreement with experimental results. Using known structural parameters of the ammonia molecule, the eigenvalues for a 10 × 10 truncation of the Hamiltonian matrix are computed. The splitting between the two lowest eigenvalues corresponds to the ammonia maser frequency, 24.87 {GHz}, this value being reproduced by the appropriate choice of ω.},
	number = {{arXiv}:1809.08178},
	institution = {{arXiv}},
	author = {Blinder, S. M.},
	urldate = {2022-06-07},
	date = {2018-09-19},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1809.08178 [physics]},
	note = {type: article},
	keywords = {Physics - Chemical Physics},
