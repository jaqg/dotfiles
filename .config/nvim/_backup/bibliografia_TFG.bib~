@article{muniz_investigating_2014,
	title = {Investigating Quantum Mechanical Tunneling at the Nanoscale via Analogy: Development and Assessment of a Teaching Tool for Upper-Division Chemistry},
	volume = {91},
	issn = {0021-9584, 1938-1328},
	url = {https://pubs.acs.org/doi/10.1021/ed400761q},
	doi = {10.1021/ed400761q},
	shorttitle = {Investigating Quantum Mechanical Tunneling at the Nanoscale via Analogy},
	abstract = {We report a novel educational activity designed to teach quantum mechanical tunneling to upper-division undergraduate students in the context of nanochemistry. The activity is based on a theoretical framework for analogy and is split into three parts that are linked pedagogically through the framework: classical ball-and-ramp system, tunneling involving a familiar substance ({NH}3 inversion), and tunneling in core/shell quantum dots. Students ﬁrst begin in the classical worldthe world within which they are most familiar, explore tunneling in the {NH}3 inversion paradigm to gain exposure to the stark diﬀerences between classical and quantum behavior, and ﬁnally extend the concept of tunneling to the nanoscale through the examination of spectroscopic results in the literature of {CdSe}/{ZnS} core/shell quantum dots. Additionally, the activity has been assessed via a mixed-methods approach using qualitative analysis of individual student interviews (pre- and postactivity) and recordings of students’ group discourse, along with small-sample statistical techniques when appropriate. Our ﬁndings suggest that students are able to successfully incorporate the language of quantum mechanical tunneling into their scientiﬁc language, and there is evidence for analogical transfer between relevant concepts within the activity.},
	pages = {1546--1556},
	number = {10},
	journaltitle = {Journal of Chemical Education},
	shortjournal = {J. Chem. Educ.},
	author = {Muniz, Marc N. and Oliver-Hoyo, Maria T.},
	urldate = {2022-06-07},
