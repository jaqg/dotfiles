@article{rossman_observing_2020,
	title = {Observing Quantum Dynamics in Real Time: An Excel-Ready Finite-Difference Algorithm for Solving the Time-Dependent Schrödinger Equation},
	volume = {97},
	issn = {0021-9584, 1938-1328},
	url = {https://pubs.acs.org/doi/10.1021/acs.jchemed.9b01138},
	doi = {10.1021/acs.jchemed.9b01138},
	shorttitle = {Observing Quantum Dynamics in Real Time},
	abstract = {We present an algorithm for solving the time-dependent Schrödinger equation that is based on the ﬁnite-diﬀerence expression of the kinetic energy operator. Students who have some knowledge of linear algebra can understand the theory used to derive the algorithm. This is because the ﬁnitediﬀerence kinetic energy matrix and the Hückel matrix for linear conjugated hydrocarbons have similar forms. We oﬀer 3 modules that can be used to solve 1dimensional time-dependent Schrödinger equations, in real time. Module 1 contains two worksheets designed to show quantum dynamics in bound potentials. Module 2 contains 2 worksheets designed to illustrate tunneling. Module 3 contains 2 worksheets set up to illustrate the eﬀect of time-dependent external ﬁelds. Further, modules 1 and 3 also include the Verlet algorithm so students can compare the quantum dynamical evolution of the systems to their classical counterpart in real time.},
	pages = {1026--1034},
	number = {4},
	journaltitle = {Journal of Chemical Education},
	shortjournal = {J. Chem. Educ.},
	author = {Rossman, Taylor A. and Parks, Zachary P. and Messina, Michael},
